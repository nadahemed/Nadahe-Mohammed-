        "#Modelisation elementaire des familles\n",
        "Familles=[]\n",
        "x=[randint(0,1) for _ in range(100)]\n",
        "print(x)\n",
        "for i in range(100):\n",
        "    if x[i]==0:\n",
        "        Familles.append('Fille')\n",
        "    else:\n",
        "        Familles.append('Garcon')\n",
        "print(Familles)\n",
        "#Calcul des frequences\n",
        "import numpy as np\n",
        "freq=np.unique(Familles,return_counts=True)\n",
        "print(freq)\n",
