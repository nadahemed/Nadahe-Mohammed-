      "cell_type": "code",
      "source": [
        "#Modelisation elementaire des familles pour 2 enfants au plus\n",
        "from random import randint\n",
        "x = [(randint(0, 1), randint(0, 1)) for _ in range(100)]\n",
        "print(\"Liste originale :\", x)\n",
        "resultat = [('Fille' if a == 0 else 'Garçon', 'Fille' if b == 0 else 'Garçon') for a, b in x]\n",
        "print( resultat)\n",
        "#Calcul des frequences\n",
        "unique, counts = np.unique(resultat, axis=0, return_counts=True)\n",
        "for u, c in zip(unique, counts):\n",
        "    print(f\"{u}: {c}\")\n",
        "labels = ['{}, {}'.format(f1, f2) for f1, f2 in unique]\n",
        "counts = counts\n",
        "# Tracer l'histogramme\n",
        "plt.bar(labels, counts, color='darkblue')\n",
        "plt.xlabel('Combinations de Fille et Garçon')\n",
        "plt.ylabel('Frequences')\n",
        "plt.title('Histog')\n",
        "plt.xticks(rotation=45)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
